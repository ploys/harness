{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["es2015", "es2017", "es2019"],
    "declaration": true,
    "strict": true,
    "pretty": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": ["lib", "node_modules"]
}
